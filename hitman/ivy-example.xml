<?xml version="1.0" encoding="ISO-8859-1"?>
<ivy-module version="2.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd"
	xmlns:m="http://ant.apache.org/ivy/maven">

	<info organisation="com.github.mrstampy" module="hit-example" status="integration" />
    
    <publications>
        <artifact name="hit-example"/>
        <artifact name="hit-example" type="pom" ext="pom" />
        <artifact name="hit-example" type="javadoc" m:classifier="javadoc" ext="jar" />
        <artifact name="hit-example" type="sources" m:classifier="sources" ext="jar" />
        <artifact name="hit-example" type="config" m:classifier="config" ext="zip" />
    </publications>

	<dependencies>
		<dependency org="com.github.mrstampy" name="hit" rev="${hit.version}"/>
	</dependencies>
</ivy-module>
